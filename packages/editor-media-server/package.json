{
  "name": "@karma.run/editor-media-server",
  "version": "0.12.1",
  "main": "./lib/cjs/index.js",
  "module": "./lib/es6/index.js",
  "types": "./lib/types/index.d.ts",
  "license": "MIT",
  "repository": "github:karmarun/karma.run-editor",
  "scripts": {
    "build": "run-p build:*",
    "build:cjs": "tsc --outDir ./lib/cjs",
    "build:es6": "tsc --module esnext --outDir ./lib/es6",
    "build:types": "tsc --declaration --declarationDir ./lib/types --emitDeclarationOnly",
    "watch": "run-s clean mkdir symlink watch:dev",
    "watch:dev": "tsc --watch --outDir ./lib/cjs --declaration --declarationDir ./lib/types --declarationMap",
    "mkdir": "shx mkdir -p ./lib/cjs ./lib/types",
    "symlink": "shx ln -s ./cjs ./lib/es6",
    "copy:license": "shx cp ../../LICENSE ./",
    "clean": "shx rm -rf ./LICENSE ./lib",
    "prepare": "run-s clean copy:license build"
  },
  "dependencies": {
    "@karma.run/editor-common": "^0.13.0",
    "@karma.run/editor-media-common": "^0.12.0",
    "busboy": "^0.2.14",
    "cloudinary": "^1.11.0",
    "mkdirp": "^0.5.1",
    "mmmagic": "^0.5.0",
    "sharp": "^0.20.3",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "@types/busboy": "^0.2.3",
    "@types/express": "^4.11.1",
    "@types/mkdirp": "^0.5.2",
    "@types/mmmagic": "^0.4.29",
    "@types/sharp": "^0.17.8",
    "@types/shortid": "^0.0.29",
    "npm-run-all": "^4.1.3",
    "shx": "^0.3.0",
    "typescript": "^2.9.1"
  },
  "peerDependencies": {
    "express": "^4.0.0"
  }
}
